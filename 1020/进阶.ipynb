   "outputs": [],
   "source": [
    "#固定长度\n",
    "#一个整数数组 固定长度k 找长度k连续子数组里面元素和max 并返回\n",
    "\n",
    "def max_sum_len_k(nums,k):\n",
    "    s = sum(nums[:k])\n",
    "    ans = s#目前max就是首个窗口和\n",
    "    #切片0-k-1 浅拷贝 求和\n",
    "\n",
    "    for i in range(k,len(nums)):\n",
    "        s+=nums[i]-nums[i-k]\n",
    "        ans = max(ans,s)\n",
    "        return ans\n"
   ]
  }
